{
  "name": "starry-ui",
  "version": "0.0.1",
  "description": "",
  "main": "lib/main.js",
  "module": "es/main.js",
  "files": [
    "lib",
    "es",
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "start": "npm run dev",
    "clean": "rimraf dist lib es",
    "build": "npm run clean && npm run build:lib && npm run build:es && npm run build:umd && npm run build:css",
    "dev": "start-storybook -p 9001 -c .storybook",
    "test": "cross-env BABEL_ENV=test jest --config jest.config.js",
    "test:update": "npm run test -u",
    "coverage": "npm run test --coverage",
    "build:docs": "build-storybook -o docs",
    "prepublish": "npm run build",
    "build:css": "cd scripts && gulp",
    "build:lib": "cross-env OUTPUT_MODULE=commonjs babel src/components -d lib --ignore **/__tests__",
    "build:es": "babel src/components -d es --ignore **/__tests__",
    "build:umd": "webpack --config ./scripts/build.umd.js"
  },
  "keywords": [
    "react",
    "react-component",
    "components",
    "framework",
    "frontend",
    "starry-ui"
  ],
