    "react",
    "react-ui",
    "components",
    "javascript",
    "es6",
    "css3",
    "starry-ui"
  ],
  "sideEffects": [
    "dist/*",
    "lib/*",
    "es/*",
    "*.styl"
  ],
  "resolutions": {
    "webpack": "5.52.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eurislee/starry_ui.git"
